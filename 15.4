class tochka:
    def __init__(self, x, y):
        self.__x=x
        self.__y=y
    @property
    def x(self):
        return self.__x

    @property
    def y(self):
        return self.__y

    def func(self, p):
        return (self.x - p.x) **3 + (self.y + p.y) ** 2

p = tochka(0, 0)
q = tochka(1, 1)
print(p.func(q))
