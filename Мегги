d = {'Молоко': 30, 'Шоколад': 50, 'вода':20, 'сок':30, 'губка':55. 'морс':80, 'вилка':90, 'шок':77, 'яблоко':15, 'жвачка':8}
while True:
    text = input("Введите товар, стоп или добавить: ")
    if text == "стоп":
        print("Всего доброго!")
        break
    elif text == "добавить":
        n = input("Введите название товара: ")
        s = input("Введите стоимость товара: ")
        d.setdefault(n, s)
    else:
        print("Стоимость товара", d[text])
